  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "NODE_ENV=production next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:all": "npm run test:coverage && npm run test:e2e",
    "prepare": "husky",
    "pre-commit": "./.husky/pre-commit",
    "pre-push": "./.husky/pre-push",
    "quality:all": "npm run lint && npm run test:coverage && npm run build",
    "hooks:check": "ls -la .husky/ && echo '✅ Husky hooks are installed'",
    "hooks:reinstall": "rm -rf .husky && npx husky init && chmod +x .husky/pre-commit && chmod +x .husky/pre-push && echo '✅ Husky hooks reinstalled'",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
